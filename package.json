  "bugs": {
    "url": "https://github.com/phoboslab/jsmpeg/issues"
  },
  "homepage": "https://github.com/phoboslab/jsmpeg#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "webpack": "^4.42.0",
